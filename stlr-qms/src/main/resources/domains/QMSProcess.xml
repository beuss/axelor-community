<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
<!--
	Represents a quality process as per ISO9001 principles.
	Will be enhanced to be able to handle input data, responsibles & so on.
	We cannot use QualityProcess name since its used in axelor-quality for a
	totally different purpose.
-->
	<module name="axelor-qms" package="com.axelor.apps.qms.db" />
	<entity name="QMSProcess" lang="java" cachable="true" sequential="true">
		<integer name="selectType" selection="qms.process.type.select" required="true" nullable="false"/>

		<string name="code" max="3" nullable="false" required="true" />
		<string name="title" nullable="false" required="true" namecolumn="true" />

		<many-to-many name="responsibles" ref="com.axelor.apps.hr.db.Employee" title="Responsibles" required="true"/>
		<many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company" required="true" />

	</entity>
</domain-models>
